#include <stdio.h>
#include <stdbool.h>

int main()
{
    bool arr[1001];
    int n;

    printf("1000이하의 수를 입력하세요.\n");
    scanf("%d",&n);

    for(int i = 1 ; i <= n ; i++)
    {
        arr[i] = true;
    }

    for(int i = 2 ; i <= n ; i++)
    {
        if(arr[i] == true)
        {
            for(int j = i+i ; j <= n ; j+=i)
            {
                arr[j] = false;
            }
        }
    }

    for(int i = 2 ; i <= n ; i++)
    {
        if(arr[i] == true)
        {
            printf("%d ",i);
        }
    }

    return 0;
}
